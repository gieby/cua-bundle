<ul id="projects">
    <li v-for="project in projects" class="entry"">
        <figure class="projectThumb">
            <img v-attr="src: project.thumbnail" />
        </figure>
        <div class="title">
            <span class="projectTitle">{{ project.title }}</span>
            <span class="projectPlace">{{ project.place }}</span>
        </div>
    </li>
</ul>